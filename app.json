      "**/*"
    ],
    "android": {},
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      "expo-screen-orientation"
    ],
    "extra": {
      "eas": {
        "projectId": "aeb87672-f7be-463d-88f5-06d777987e4d"
      }
    },
    "owner": "bendavidi , alonkman"
  },
  "web": {
    "favicon": "./assets/favicon.png"
  }
}
